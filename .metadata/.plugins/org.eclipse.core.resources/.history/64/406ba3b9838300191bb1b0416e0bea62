
public class main {

	public static void main(String[] args) {
		Person p = new Person("Tor", 33);
		
		Bar[] bars = new Bar[2];
		bars[0] = new Bar("The Asgardian", 25, new String[]{"Loke", "Freja"});
		bars[1] = new Bar("Loke's tavern", 0, new String[] {"Tor", "Oden"});
	
		bars[0].enter(p);
	}

}
