
public class Bars {
	private class Bar{
		String name;
		int ageRequirement;
		String[] banned;
		
		public Bar(String name, int ageReq, String[] banned) {
			this.name = name;
			ageRequirement = ageReq;
			this.banned = banned;
		}
		
		public boolean checkAge(int age) {
			return age >= ageRequirement;
		}
	}
}
