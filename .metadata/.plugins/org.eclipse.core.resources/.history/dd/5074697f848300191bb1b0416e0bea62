
public class main {

	public static void main(String[] args) {
		Person p1 = new Person("Tor", 33);
		Person p2 = new Person("Loke", 25);
		
		Bar[] bars = new Bar[2];
		bars[0] = new Bar("The Asgardian", 25, new String[]{"Loke", "Freja"});
		bars[1] = new Bar("Freja's tavern", 0, new String[] {"Tor", "Oden"});

		bars[0].enter(p1);
		System.out.println();
		
		bars[0].enter(p2);
		System.out.println();
		//bars[1].enter(p1);
	}

}
