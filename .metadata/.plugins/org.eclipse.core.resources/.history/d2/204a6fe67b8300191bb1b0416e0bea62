
public class Person {
	int age;
	String name;
	int soc_sec;
	
	public Person(String n, int a, int s_s) {
		age = a;
		name = n;
		soc_sec = s_s;
	}
	
	public void happy_birthday() {
		age = age + 1;
	}
	
	public int getAge() {
		return age;
	}
	
	public void printInfo() {
		System.out.println("Name: " + name);
		System.out.println("Age: " + age);
		System.out.println("Social security: " + soc_sec);
	}
}
